{
  "name": "{{package_name}}",
  "version": "{{api_version}}",
  "description": "{{#if description}}{{description}}{{else}}TypeScript gRPC SDK for {{api_name}}{{/if}}",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./web": {
      "types": "./dist/web.d.ts",
      "import": "./dist/web.mjs",
      "require": "./dist/web.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "proto/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "proto:generate": "protoc --ts_out=src/generated --proto_path=proto proto/*.proto",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "grpc",
    "protobuf",
    "api",
    "client",
    "sdk",
    "typescript",
    "{{package_name}}"
  ],
  "author": "Generated by client-sdk-generator",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.0",
    "grpc-web": "^1.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "protoc-gen-ts": "^0.8.0",
    "ts-jest": "^29.0.0",
    "ts-proto": "^1.156.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/example/{{package_name}}.git"
  },
  "bugs": {
    "url": "https://github.com/example/{{package_name}}/issues"
  },
  "homepage": "https://github.com/example/{{package_name}}#readme"
}